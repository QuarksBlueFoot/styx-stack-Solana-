{
  "name": "@styxstack/whisperdrop-sdk",
  "version": "1.0.2",
  "description": "TypeScript SDK for WhisperDrop - Privacy-Preserving Airdrops on Solana",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "airdrop",
    "privacy",
    "merkle",
    "token-gating",
    "nft",
    "web3",
    "whisperdrop"
  ],
  "author": "@moonmanquark <Bluefoot Labs>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuarksBlueFoot/StyxStack.git",
    "directory": "packages/whisperdrop-sdk"
  },
  "homepage": "https://github.com/QuarksBlueFoot/StyxStack/tree/main/packages/whisperdrop-sdk#readme",
  "peerDependencies": {
    "@solana/web3.js": "^1.87.0 || ^2.0.0",
    "react-native": ">=0.70.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "dependencies": {
    "@noble/hashes": "^1.5.0"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.98.0",
    "@solana/spl-token": "^0.4.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
