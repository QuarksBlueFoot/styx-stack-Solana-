{
  "name": "@styxstack/sps-sdk",
  "version": "3.0.0",
  "description": "Styx Privacy Standard (SPS) SDK - Privacy-native tokens, NFTs, and DeFi on Solana",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./domains": {
      "import": "./dist/domains.mjs",
      "require": "./dist/domains.js",
      "types": "./dist/domains.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/domains.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/domains.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "solana",
    "privacy",
    "token",
    "sps",
    "styx",
    "nft",
    "defi",
    "confidential",
    "inscription"
  ],
  "author": "Bluefoot Labs",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuarksBlueFoot/StyxStack"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.87.0 || ^2.0.0",
    "react-native": ">=0.70.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "dependencies": {
    "@noble/ciphers": "^0.4.1",
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.3",
    "@solana/spl-token": "^0.4.0"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.87.0",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}
