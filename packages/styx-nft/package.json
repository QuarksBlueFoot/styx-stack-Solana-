{
    "name": "@styxstack/styx-nft",
    "version": "1.0.0",
    "description": "ANONFT - Anonymous NFT Standard: Privacy-first NFTs with ring signatures, ZK proofs, and Metaplex compatibility",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./forest": {
            "types": "./dist/forest.d.ts",
            "import": "./dist/forest.js",
            "require": "./dist/forest.js"
        },
        "./ring": {
            "types": "./dist/ring.d.ts",
            "import": "./dist/ring.js",
            "require": "./dist/ring.js"
        },
        "./zk": {
            "types": "./dist/zk.d.ts",
            "import": "./dist/zk.js",
            "require": "./dist/zk.js"
        },
        "./bridge": {
            "types": "./dist/bridge.d.ts",
            "import": "./dist/bridge.js",
            "require": "./dist/bridge.js"
        }
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "test": "vitest run",
        "test:watch": "vitest",
        "lint": "eslint src/",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@coral-xyz/anchor": "^0.30.1",
        "@metaplex-foundation/mpl-bubblegum": "^4.0.0",
        "@metaplex-foundation/mpl-token-metadata": "^3.2.0",
        "@metaplex-foundation/umi": "^1.0.0",
        "@noble/curves": "^1.8.2",
        "@noble/hashes": "^1.7.2",
        "@solana/spl-account-compression": "^0.4.1",
        "@solana/spl-token": "^0.4.6",
        "@solana/web3.js": "^1.95.4",
        "poseidon-lite": "^0.3.0"
    },
    "devDependencies": {
        "@types/node": "^22.10.10",
        "typescript": "^5.7.3",
        "vitest": "^2.1.8"
    },
    "keywords": [
        "solana",
        "nft",
        "privacy",
        "ring-signature",
        "zero-knowledge",
        "metaplex",
        "compressed-nft",
        "phantom",
        "styx"
    ],
    "license": "MIT"
}
